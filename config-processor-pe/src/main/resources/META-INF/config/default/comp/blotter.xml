<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://baxter-it.com/config/pe/blotter">

    <priceDisplay currencyPairsPerRow="8" currencyPairs="EU" active="true"> </priceDisplay>

    <ui>
        <flag id="AtTrade">true</flag>
        <flag id="AtPending">true</flag>
        <flag id="AtMRR">true</flag>
        <flag id="AtNEG">true</flag>
        <flag id="salesBlotter">true</flag>
        <flag id="tradingTool">true</flag>
        <flag id="useDIPanel">false</flag>

        <!-- If does not present then showFailedTradePopup=false -->
        <!-- Possible positions: CENTER, SOUTH_EAST, SOUTH_WEST, NORTH_WEST, NORTH_EAST -->
        <failedTrade position="SOUTH_EAST"/>

        <sound id="missedTrade">res\alert.wav</sound>
        <sound id="failedTrade">res\alert.wav</sound>


        <tabAfterDI>Trading Tool</tabAfterDI>
        <countdownSeconds>30</countdownSeconds>

    </ui>

</configuration>

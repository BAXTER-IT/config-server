<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender class="ch.qos.logback.core.ConsoleAppender" name="CONSOLE">
        <encoder>
            <pattern>%d{ISO8601}  [%5p] %c{1} %m%n</pattern>
        </encoder>
    </appender>
    
    <logger name="com.baxter" level="INFO">
    </logger>
    
    <root level="INFO">
        <appender-ref ref="CONSOLE" />
    </root>
    
</configuration>
